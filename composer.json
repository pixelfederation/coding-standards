{
  "name": "pixelfederation/coding-standards",
  "homepage": "https://git.pixelfederation.com/shared/server-coding-standards",
  "type": "library",
  "description": "Standardized PHPCS sniffs for backend developers.",
  "license": "proprietary",
  "authors": [
    {
      "name": "Martin Fris",
      "email": "mfris@pixelfederation.com"
    }
  ],
  "repositories": [
    {
      "type": "composer",
